{
  "version": 2,
  "name": "omax",
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(dashboard|jobs|settings|costs|inventory)(?:/.*)?", "dest": "/index.html" }
  ]
}
